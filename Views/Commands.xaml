<UserControl x:Class="Supermarket.Views.Commands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Supermarket.Views"
             mc:Ignorable="d" 
             d:DesignHeight="180" d:DesignWidth="140">
    <Grid Background="White">
        <Button Content="Add" Width="100" Height="40" Margin="20,20,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button Content="Edit" Width="100" Height="40" Margin="20,70,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <Button Content="Delete" Width="100" Height="40" Margin="20,120,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
    </Grid>
